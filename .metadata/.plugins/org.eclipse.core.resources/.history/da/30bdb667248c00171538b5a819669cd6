package sortAlfabet;

public class SortForAlfabet {

	public static void sort(char[] array) {

	}

	public static void main(String[] args) {
		String stroka = "qwertyu";
		char[] array = stroka.toCharArray();
		System.out.println(array);
		for (int i = 0; i < array.length; i++) {
			for (int j = i; j < array.length; j++) {
				if (array[i] > array[j]) {
					char temp = array[i];
					array[i] = array[j];
					array[j] = temp;
				}
			}
		}

		System.out.println(array);
		String stroka2 = new String(array);
		System.out.println(stroka2);
	}

}
